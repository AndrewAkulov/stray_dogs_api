<div class="nested-fields">
  <%= f.collection_select(:breed_id, Breed.all, :id, :name, { include_blank:"Select breed" }, required: true, class: 'form-control mb-2 mr-sm-2' ) %>


  <%= f.collection_select(:city_id, City.all, :id, :name, { include_blank:"Select city" }, required: true, class: 'form-control mb-2 mr-sm-2' ) %>

  
  <%= f.collection_select(:age_from, Age.all, :id, :years, { include_blank:"Select age" }, required: true, class: 'form-control mb-2 mr-sm-2' ) %>

 
  <%= f.collection_select(:age_to, Age.all, :id, :years, { include_blank:"Select age" }, required: true, class: 'form-control mb-2 mr-sm-2' ) %>
  <%= link_to_remove_association image_tag("cancel.png", class: "add-subscribe-btn mb-2 mr-sm-2"), f %>
  <%=  %>
</div>
 
    
    
  
